navigator.getVRDisplays().then(function(displays){
if(displays.length){
_this6.setCurrentDisplay(displays[0]);
return _this6.enterVR();
}
}).catch(function(err){
// Silence the permissions error, it's typically expected
});
}
}},
{
key:'onDisplayConnect',
value:function onDisplayConnect(_ref){
var display=_ref.display;

if(this.vrDisplay){
return;
}
this.setCurrentDisplay(display);
}},
{
key:'onDisplayDisconnect',
value:function onDisplayDisconnect(_ref2){
var _this7=this;

var display=_ref2.display;

// if display is not current display, return
// if presenting, exit presenting, cleanup
// query remaining displays
// if displays are not empty, set current display to first one
// else, clear current display, set state to disconnected
if(display!==this.vrDisplay){
return;
}
if(typeof navigator.getVRDisplays==='function'){
navigator.getVRDisplays().then(function(displays){
if(displays.length===0){
_this7.setCurrentDisplay(displays[0]);
}else{
_this7.setCurrentDisplay(null);
}
});
}
}},
{
key:'onDisplayActivate',
value:function onDisplayActivate(_ref3){
var display=_ref3.display;

if(display===this.vrDisplay){
this.enterVR();
}
}},
{
key:'onDisplayDeactivate',
value:function onDisplayDeactivate(_ref4){
var display=_ref4.display;

if(display===this.vrDisplay){
this.exitVR();
}
}

// Get/Set camera needed so the default camera can be retrieved or adjusted.
},
{
key:'camera',
get:function get(){
return this._camera;
},
set:function set(value){
this._camera=value;
this._initialAngles={
x:value.rotation.x,
y:value.rotation.y,
z:value.rotation.z};

if(typeof this.controls.setCamera==='function'){
this.controls.setCamera(value);
}
}

// Get renderer
},
{
key:'renderer',
get:function get(){
return this.glRenderer;
}}]);


return Player;
}();

exports.default=Player;